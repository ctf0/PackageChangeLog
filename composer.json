{
    "name": "ctf0/package-changelog",
    "description": "show package changeLog on install/update",
    "homepage": "https://github.com/ctf0/PackageChangeLog",
    "license": "MIT",
    "keywords": [
        "package-changelog"
    ],
    "authors": [
        {
            "name": "Iven Simon",
        }
    ],
    "require": {
        "illuminate/support": ">=7.0 <=11.0"
    },
    "autoload": {
        "psr-4": {
            "ctf0\\PackageChangeLog\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ctf0\\PackageChangeLog\\PackageChangeLogServiceProvider"
            ]
        },
        "changeLog": "logs"
    },
    "config": {
        "sort-packages": true
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/ctf0"
        }
    ]
}
