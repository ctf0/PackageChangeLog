{
    "name": "ivenms/package-changelog",
    "description": "show package changeLog on install/update",
    "homepage": "https://github.com/ivenms/PackageChangeLog",
    "license": "MIT",
    "keywords": [
        "package-changelog"
    ],
    "authors": [
        {
            "name": "Iven Simon"
        }
    ],
    "require": {
        "illuminate/support": "9.0 - 11.0"
    },
    "autoload": {
        "psr-4": {
            "ivenms\\PackageChangeLog\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ivenms\\PackageChangeLog\\PackageChangeLogServiceProvider"
            ]
        },
        "changeLog": "logs"
    },
    "config": {
        "sort-packages": true
    }
}
